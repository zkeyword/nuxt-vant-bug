{
  "version": "1.0.0",
  "name": "CDS",
  "author": "CDS F2E",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "build:test": "cross-env NODE_ENV_OSS=test nuxt build",
    "build:prod": "cross-env NODE_ENV_OSS=prod nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .js components/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "cross-env NODE_ENV_OSS=test npm run build && cross-env NODE_ENV_OSS=test node ./upload.js && npm run start",
    "prod": "cross-env NODE_ENV_OSS=prod npm run build && cross-env NODE_ENV_OSS=prod node ./upload.js && npm run start",
    "deploy": "npm run build && node ./upload.js && npm run start"
  },
  "dependencies": {
    "@base/storage": "^0.2.7",
    "@base/vconsole": "^1.0.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "ant-design-vue": "^1.4.11",
    "better-scroll": "^1.15.2",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^5.2.0",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.8.20",
    "echarts": "^4.6.0",
    "element-ui": "^2.13.0",
    "highcharts": "^8.0.2",
    "highcharts-vue": "^1.3.5",
    "iview": "^3.1.5",
    "js-cookie": "^2.2.1",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^8.0.8",
    "lru-cache": "^5.1.1",
    "nuxt": "^2.0.0",
    "ramda": "^0.27.0",
    "timezone-support": "^2.0.2",
    "vant": "^2.5.5",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-truncate-collapsed": "^2.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/router": "^1.5.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "ali-oss": "^6.5.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.13.0",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^4.2.1",
    "vsvg": "^1.9.1",
    "vue-svg-loader": "^0.16.0"
  }
}
